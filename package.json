{
  "name": "@sign/i18n",
  "version": "0.0.1",
  "title": "i18n",
  "color": "black",
  "icon": {
    "emoji": "🌐"
  },
  "description": "Shared i18n names for common types like countries and languages",
  "repository": "https://github.com/sign/i18n.git",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "prettier": "./node_modules/@sign/configuration/.prettierrc.json",
  "scripts": {
    "build": "node build.js",
    "prepare:husky": "node -e \"require('fs').cpSync('node_modules/@sign-mt/configuration/.husky/', '.husky/', {recursive: true})\"",
    "prepare": "husky install && npm run prepare:husky"
  },
  "devDependencies": {
    "@sign/configuration": "git://github.com/sign/.github.git",
    "csvtojson": "^2.0.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  }
}
